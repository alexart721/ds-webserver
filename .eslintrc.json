{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-typescript/base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "ds-webserver/tsconfig.json"
    },
    "rules": {
        "no-console":"off",
        "consistent-return":"off",
        "import/prefer-default-export": "off",
        "max-len": "off",
        "no-underscore-dangle": "off"
    }
}
